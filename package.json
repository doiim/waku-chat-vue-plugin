{
  "name": "waku-chat-vue-plugin",
  "type": "module",
  "homepage": "https://doiim.github.io/waku-chat-vue-plugin/",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/waku-chat-vue-plugin.umd.cjs",
  "module": "./dist/waku-chat-vue-plugin.js",
  "exports": {
    ".": {
      "import": "./dist/waku-chat-vue-plugin.js",
      "require": "./dist/waku-chat-vue-plugin.umd.cjs"
    }
  },
  "files": [
    "dist",
    "!src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build"
  },
  "dependencies": {
    "@libp2p/bootstrap": "^10.0.16",
    "@libp2p/interfaces": "^3.3.2",
    "@waku/sdk": "^0.0.22",
    "protobufjs": "^7.2.6",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
